{
  "name": "Petlab Funnel Browser Tool",
  "description": "Petlab Funnel Browser Tool",
  "version": "1.0.3",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "96": "/images/96.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "/images/96.png",
      "default_title": "Insert Shortcuts"
    },
  "homepage_url": "https://github.com/chesteralan/petlab-funnel-browser-tool",
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": false
  },
  "web_accessible_resources": [
    "images/pencil.svg",
    "images/gears.svg",
    "images/desktop.svg",
    "images/globe.svg",
    "images/dev.svg",
    "images/cogs.svg"
  ]
}
