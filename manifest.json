{
  "name": "Petlab Funnel Browser Tool",
  "description": "Petlab Funnel Browser Tool",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "96": "/images/96.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content-script.js"]
    }
  ]
}
