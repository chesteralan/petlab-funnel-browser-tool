{
  "name": "Petlab Funnel Browser Tool",
  "description": "Petlab Funnel Browser Tool",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "96": "/images/96.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "/images/96.png",
      "default_title": "Insert Shortcuts"
    }
}
